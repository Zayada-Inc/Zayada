{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "/images/*.{png,jpg,gif}", "/css/*", "/js/*" ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/login",
      "redirect": "/login",
      "statusCode": 301
    }
  ],
  "mimeTypes": {
    "webmanifest": "application/manifest+json"
  }
}